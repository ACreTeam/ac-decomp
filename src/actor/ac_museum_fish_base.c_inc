    fishActor->_612.x =
        atans_table(fishActor->_5E8, fishActor->_5F4 + fishActor->init_data._0C - fishActor->position.y) >> 4;
    fishActor->_5D0.y = 0;

    if (RANDOM_F(1) > fishActor->init_data._1C) {
